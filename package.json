{
	"name": "bfs",
	"displayName": "BFS",
	"description": "services for BFS",
	"publisher": "bfchain",
	"version": "0.0.3",
	"keywords": [
		"bfs",
		"pkgm",
		"bfsp",
		"bfsw",
		"bfchain"
	],
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.10.1"
	},
	"categories": [
		"Other"
	],
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.1.1",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"typescript": "^4.6.4",
		"@vscode/test-electron": "^2.1.3"
	},
	"activationEvents": [
		"workspaceContains:**#bfsp.ts",
		"workspaceContains:**#bfsw.ts"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "BFS",
			"properties": {
				"PKGM.excludes": {
					"type": "object",
					"default": {
						"**/*.json": true,
						"**/.*": true,
						"**/*.log": true,
						"**/*.lock": true,
						"**/tsconfig*": true
					},
					"description": "pkgm项目需要隐藏的文件"
				}
			}
		},
		"commands": [
			{
				"command": "BFS.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"homepage": "https://github.com/waterbang/bfs-vscode",
	"bugs": {
		"url": "https://github.com/waterbang/bfs-vscode/issues",
		"email": "water_bang1@163.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/waterbang/bfs-vscode.git"
	}
}